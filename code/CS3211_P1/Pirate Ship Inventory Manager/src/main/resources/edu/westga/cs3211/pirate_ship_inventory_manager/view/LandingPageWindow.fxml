<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" minHeight="343.0" minWidth="255.0" prefHeight="410.0" prefWidth="358.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.westga.cs3211.pirate_ship_inventory_manager.view.LandingPageWindow">
   <children>
      <VBox alignment="CENTER" layoutX="21.0" layoutY="138.0" prefHeight="200.0" prefWidth="316.0" spacing="30.0">
         <children>
            <Button fx:id="addStockButton" mnemonicParsing="false" onAction="#addStock" prefHeight="62.0" prefWidth="148.0" text="Add Stock">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <Button fx:id="reViewStockChangesButton" mnemonicParsing="false" onAction="#reviewStockChanges" text="Review Stock Changes">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <Button fx:id="viewInventoryButton" mnemonicParsing="false" onAction="#viewInventory" prefHeight="51.0" prefWidth="202.0" text="View Inventory">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
         </children>
      </VBox>
      <Button layoutX="252.0" layoutY="14.0" mnemonicParsing="false" onAction="#onLogoutClick" prefHeight="35.0" prefWidth="85.0" text="Logout">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
